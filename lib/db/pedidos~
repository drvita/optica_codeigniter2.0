CREATE TABLE IF NOT EXISTS `pedidos` (
	`id` int(11) NOT NULL AUTO_INCREMENT,
	`date` datetime NOT NULL,
	`fentrega` date NOT NULL,
	`test` int(11) NOT NULL,
	`idcliente` int(11) NOT NULL,
	`armazon_name` varchar(250) COLLATE utf8_spanish_ci NOT NULL,
	`armazon_code` varchar(20) COLLATE utf8_spanish_ci NOT NULL,
	`armazon` float(8,2) NOT NULL,
	`consulta` float(8,2) NOT NULL,
	`lente` float(8,2) NOT NULL,
	`lente_contacto` float(8,2) NOT NULL,
	`subtotal` float(8,2) NOT NULL,
	`descuento` float(8,2) NOT NULL,
	`anticipo` float(8,2) NOT NULL,
	`metodopago` varchar(30) COLLATE utf8_spanish_ci NOT NULL,
	`banco` text COLLATE utf8_spanish_ci NOT NULL,
	`total` float(8,2) NOT NULL,
	`observaciones` text COLLATE utf8_spanish_ci NOT NULL,
	`laboratorio` varchar(200) COLLATE utf8_spanish_ci NOT NULL,
	`npedidolab` varchar(12) COLLATE utf8_spanish_ci NOT NULL,
	`ncaja` int(11) NOT NULL,
	`mensajes` text COLLATE utf8_spanish_ci NOT NULL,
	`items` text COLLATE utf8_spanish_ci NOT NULL,
	`user` text COLLATE utf8_spanish_ci NOT NULL,
	`status` int(2) NOT NULL
) ENGINE=InnoDB AUTO_INCREMENT=1 DEFAULT CHARSET=utf8 COLLATE=utf8_spanish_ci;
